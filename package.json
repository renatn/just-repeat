{
  "name": "react-flashcards",
  "version": "1.0.0",
  "description": "Flashcards",
  "main": "app/index.js",
  "scripts": {
    "lint": "eslint app --ext=jsx --ext=js --fix",
    "compile": "webpack --config webpack.config.prod.js",
    "start": "webpack-dev-server --host 0.0.0.0 --inline --port 8080 --progress --color --content-base public/",
    "tarball": "zip -9 -r -q react-flashcards.zip public/",
    "dist": "npm run compile && npm run tarball",
    "test": "mocha --globals localStorage --compilers js:babel-core/register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/renatn/react-flashcards.git"
  },
  "keywords": [
    "flashcards",
    "react",
    "babel"
  ],
  "author": "Renat Nasyrov",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/renatn/react-flashcards/issues"
  },
  "homepage": "https://github.com/renatn/react-flashcards#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.13.1",
    "node-uuid": "^1.4.7",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "mocha": "^2.5.3",
    "redux-mock-store": "^1.0.3",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
